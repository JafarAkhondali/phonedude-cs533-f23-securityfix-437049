// Define a variable named total and make it equal to 0.
let total = 0;

// Define a variable named limit and make it equal to 10.
let limit = 10;

// Create a for loop with a variable i starting at 0 and increasing by 1 each time through the loop.
for (let i = 0; i < limit; i++) {
  // On each iteration of the loop, add the number i to the total variable.
  total += i;
}

// Print the total variable to the terminal using console.log().
console.log(total);
